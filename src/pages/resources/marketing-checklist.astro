---
import Header from "../../components/Header.astro";
import FunnelLayout from "../../layouts/FunnelLayout.astro";
import marketingChecklist from "../../data/marketing-checklist";
import DownloadCTA from "../../components/DownloadCTA.astro";
import GenericCTA from "../../components/GenericCTA.astro";

const { data } = marketingChecklist;
---

<FunnelLayout
  siteTitle={data.pageTitle}
  siteDescription={data.pageDescription}
  keywords={data.pageKeywords}
>
  <!-- HEADER -->
  <Header data={data.header} />

  <DownloadCTA data={data.downloadCTA} />

  <GenericCTA data={data.keywordsCTA} />
</FunnelLayout>
