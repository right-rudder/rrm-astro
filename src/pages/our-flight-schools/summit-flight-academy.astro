---
import ContentLong from "../../components/ContentLong.astro";
import FlyWithUsCTA from "../../components/FlyWithUsCTA.astro";
import GenericCTA from "../../components/GenericCTA.astro";
import HeaderScreen from "../../components/HeaderScreen.astro";
import SingleTestimonial from "../../components/SingleTestimonial.astro";
import summitFlightAcademy from "../../data/summit-flight-academy";
import BaseLayout from "../../layouts/BaseLayout.astro";
const { data } = summitFlightAcademy;
---

<BaseLayout
  siteTitle={data.pageTitle}
  siteDescription={data.pageDescription}
  keywords={data.pageKeywords}
>
  <!-- HEADER -->
  <HeaderScreen content={data.header} />
  <ContentLong data={data.caseLong} />
  <SingleTestimonial data={data.testimonial} />
  <FlyWithUsCTA data={data.flyWithUsCTA} />
</BaseLayout>
